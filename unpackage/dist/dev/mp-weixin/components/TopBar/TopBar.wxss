.topbar .topbar-fixed {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
  width: 100%;
  background-color: #016057;
}
.topbar .topbar-fixed .topbar-content {
  height: 45px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 15px;
  box-sizing: border-box;
}
.topbar .topbar-fixed .topbar-content .topbar-serch {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 100%;
  height: 30px;
  background-color: #fff;
  border-radius: 30px;
  padding: 0 15px;
}
.topbar .topbar-fixed .topbar-content .topbar-serch .topbar-serch-icon {
  width: 10px;
  height: 10px;
  border: 1px solid red;
  margin-right: 10px;
}
.topbar .topbar-fixed .topbar-content .topbar-serch .topbar-serch-text {
  font-size: 12px;
  color: #999;
}

