.tab {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 100%;
  border-bottom: 1px solid #f5f5f5;
  background-color: #fff;
  box-sizing: border-box;
}
.tab .tab-scroll {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  overflow: hidden;
}
.tab .tab-scroll .tab-scroll-container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  height: 45px;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  box-sizing: border-box;
}
.tab .tab-scroll .tab-scroll-container .tab-scroll-item {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  padding: 0 10px;
  font-size: 14px;
  color: #333;
}
.tab .tab-icon {
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  width: 45px;
}
.tab .tab-icon .icon {
  color: #666;
  font-size: 26px;
}
.tab .tab-icon::after {
  content: "";
  position: absolute;
  top: 12px;
  bottom: 12px;
  left: 0;
  width: 1px;
  background-color: #ddd;
}

